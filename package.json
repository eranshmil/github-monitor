{
  "name": "github-monitor",
  "version": "0.0.0",
  "engines": {
    "node": ">=8.0.0 <=11.3.0",
    "yarn": "^1.0.0"
  },
  "main": "dist/server.js",
  "scripts": {
    "lint": "ng lint",
    "build:client": "ng build --prod",
    "build:server": "webpack",
    "dev:client": "ng serve --open",
    "dev:server": "webpack --watch",
    "dev:server:brk": "INSPECT_BRK=true webpack --watch",
    "dev": "concurrently -n client,server \"yarn dev:client\" \"yarn dev:server\"",
    "build": "concurrently -n client,server \"yarn build:client\" \"yarn build:server\"",
    "format": "prettier --write \"src/**/*.{ts,scss,html}\"",
    "heroku-postbuild": "yarn build",
    "start": "node ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.1",
    "@angular/cdk": "^7.2.1",
    "@angular/cli": "~7.2.2",
    "@angular/common": "~7.2.1",
    "@angular/compiler": "~7.2.1",
    "@angular/compiler-cli": "~7.2.1",
    "@angular/core": "~7.2.1",
    "@angular/forms": "~7.2.1",
    "@angular/material": "^7.2.1",
    "@angular/platform-browser": "~7.2.1",
    "@angular/platform-browser-dynamic": "~7.2.1",
    "@angular/router": "~7.2.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "core-js": "^2.6.2",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "hammerjs": "^2.0.8",
    "mongoose": "^5.4.5",
    "octicons": "^8.3.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "typescript": "~3.2.4",
    "winston": "^3.1.0",
    "zone.js": "~0.8.28"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.2",
    "@angular/language-service": "~7.2.1",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/jasmine": "~3.3.7",
    "@types/jasminewd2": "~2.0.3",
    "@types/mongoose": "^5.3.8",
    "@types/node": "~10.12.18",
    "awesome-typescript-loader": "^5.2.1",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.1.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "nodemon-webpack-plugin": "^4.0.7",
    "prettier": "^1.16.0",
    "protractor": "~5.4.2",
    "ts-node": "~7.0.0",
    "tsconfig-paths": "^3.7.0",
    "tslint": "~5.12.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}